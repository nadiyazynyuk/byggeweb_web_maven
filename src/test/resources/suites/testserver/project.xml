<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Byggeweb Project UI tests">
    <parameter name="baseUrl"
               value="https://testserver.byggeweb.dk/controller?cmd=company7.login.form&amp;language=English"/>

    <test name="Project navigation tests">
        <parameter name="testData" value="testserver/project/launchProjectTestData.xml"/>
        <classes>
            <class name="dk.byggeweb.project.navigation.BasicNavigation"/>
            <class name="dk.byggeweb.project.navigation.WorkspaceNavigation"/>
            <class name="dk.byggeweb.project.navigation.PublicationSpaceNavigation"/>
        </classes>
    </test>

    <test name="Workspace folders tests">
        <parameter name="testData" value="testserver/project/workspace/createNewFolderTestData.xml"/>
        <classes>
            <class name="dk.byggeweb.project.workspace.folders.CreateNewFolder"/>
        </classes>
    </test>

    <test name="Workspace enhanced upload single file tests">
        <parameter name="testData" value="testserver/project/workspace/enhancedUploadSingleFileTestData.xml"/>
        <classes>
            <class name="dk.byggeweb.project.workspace.files.upload.EnhancedUploadSingleFile"/>
        </classes>
    </test>

    <test name="Workspace standard upload single file tests">
        <parameter name="testData" value="testserver/project/workspace/standardUploadSingleFileTestData.xml"/>
        <classes>
            <class name="dk.byggeweb.project.workspace.files.upload.StandardUploadSingleFile"/>
        </classes>
    </test>

    <test name="Workspace transfer single file tests">
        <parameter name="testData" value="testserver/project/workspace/transferFileTestData.xml"/>
        <classes>
            <class name="dk.byggeweb.project.workspace.files.upload.TransferSingleFile"/>
        </classes>
    </test>

    <test name="Workspace open file version test">
        <parameter name="testData" value="testserver/project/workspace/openFileVersionTestData.xml"/>
        <classes>
            <class name="dk.byggeweb.project.workspace.files.open.OpenFileVersion"/>
        </classes>
    </test>

    <test name="Workspace edit file information tests">
        <parameter name="testData" value="testserver/project/workspace/editFileInformationTestData.xml"/>
        <classes>
            <class name="dk.byggeweb.project.workspace.files.update.EditFileInformation"/>
        </classes>
    </test>

    <test name="Delete file from Workspace">
        <parameter name="testData" value="testserver/project/workspace/deleteFileTestData.xml"/>
        <classes>
            <class name="dk.byggeweb.project.workspace.files.delete.DeleteFile"/>
        </classes>
    </test>

    <test name="Recycle file from Workspace">
        <parameter name="testData" value="testserver/project/workspace/recycleFileTestData.xml"/>
        <classes>
            <class name="dk.byggeweb.project.workspace.files.delete.DeleteFileViaRecycleBin"/>
        </classes>
    </test>

    <test name="Restore recycled file from Workspace">
        <parameter name="testData" value="testserver/project/workspace/restoreRecycledFileTestData.xml"/>
        <classes>
            <class name="dk.byggeweb.project.workspace.files.delete.RestoreFileFromRecycleBin"/>
        </classes>
    </test>

    <test name="Move file to another folder in Workspace">
        <parameter name="testData" value="testserver/project/workspace/moveFileTestData.xml"/>
        <classes>
            <class name="dk.byggeweb.project.workspace.files.update.MoveFile"/>
        </classes>
    </test>

    <test name="Copy file to another folder in Workspace">
        <parameter name="testData" value="testserver/project/workspace/copyFileTestData.xml"/>
        <classes>
            <class name="dk.byggeweb.project.workspace.files.update.CopyFile"/>
        </classes>
    </test>

    <test name="Lock/unlock file in Workspace">
        <parameter name="testData" value="testserver/project/workspace/lockFileTestData.xml"/>
        <classes>
            <class name="dk.byggeweb.project.workspace.files.update.LockFile"/>
        </classes>
    </test>

    <test name="Start/stop file monitoring in Workspace">
        <parameter name="testData" value="testserver/project/workspace/monitorFileTestData.xml"/>
        <classes>
            <class name="dk.byggeweb.project.workspace.files.update.MonitorFile"/>
        </classes>
    </test>

    <test name="Download single file in Workspace">
        <parameter name="testData" value="testserver/project/workspace/downloadFileTestData.xml"/>
        <classes>
            <class name="dk.byggeweb.project.workspace.files.download.DownloadFile"/>
        </classes>
    </test>

    <test name="Download multiple files in Workspace">
        <parameter name="testData" value="testserver/project/workspace/downloadFilesTestData.xml"/>
        <classes>
            <class name="dk.byggeweb.project.workspace.files.download.DownloadFiles"/>
        </classes>
    </test>

    <test name="Publish new file from Workspace">
        <parameter name="testData" value="testserver/project/workspace/publishNewFileTestData.xml"/>
        <classes>
            <class name="dk.byggeweb.project.workspace.files.publish.PublishFile"/>
        </classes>
    </test>

    <test name="Publish new file version from Workspace">
        <parameter name="testData" value="testserver/project/workspace/publishNewFileVersionTestData.xml"/>
        <classes>
            <class name="dk.byggeweb.project.workspace.files.publish.PublishNewFileVersion"/>
        </classes>
    </test>

    <test name="Publish new file from Local Machine into Publication space">
        <parameter name="testData" value="testserver/project/publicationspace/publishNewFileFromLocalMachineTestData.xml"/>
        <classes>
            <class name="dk.byggeweb.project.publicationspace.files.publish.PublishNewFileFromLocalMachine"/>
        </classes>
    </test>

    <test name="Publish new file version from Local Machine into Publication space">
        <parameter name="testData" value="testserver/project/publicationspace/publishNewFileVersionFromLocalMachineTestData.xml"/>
        <classes>
            <class name="dk.byggeweb.project.publicationspace.files.publish.PublishNewFileVersionFromLocalMachine"/>
        </classes>
    </test>

</suite>